<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <ResourceDictionary.ThemeDictionaries>

    <ResourceDictionary x:Key="Light">
      <SolidColorBrush x:Key="Color1">#D9D9D9</SolidColorBrush>
      <SolidColorBrush x:Key="Color2">#556f7b</SolidColorBrush>
      <SolidColorBrush x:Key="Color3">#455f6b</SolidColorBrush>
      <SolidColorBrush x:Key="Color4">#005f7f</SolidColorBrush>
      <SolidColorBrush x:Key="Color5">#FF2222</SolidColorBrush>

      <LinearGradientBrush x:Key="Background" StartPoint="0,0" EndPoint="100,1000">
        <GradientStop Offset="0" Color="#ebf2f4" />
        <GradientStop Offset="1" Color="#455f6b" />
      </LinearGradientBrush>
    </ResourceDictionary>

    <ResourceDictionary x:Key="Dark">
      <SolidColorBrush x:Key="Color1">#38393C</SolidColorBrush>
      <SolidColorBrush x:Key="Color2">#556f7b</SolidColorBrush>
      <SolidColorBrush x:Key="Color3">#455f6b</SolidColorBrush>
      <SolidColorBrush x:Key="Color4">#005f7f</SolidColorBrush>
      <SolidColorBrush x:Key="Color5">#FF2222</SolidColorBrush>

      <LinearGradientBrush x:Key="Background" StartPoint="0,0" EndPoint="100,1000">
        <GradientStop Offset="0" Color="#ebf2f4" />
        <GradientStop Offset="1" Color="#455f6b" />
      </LinearGradientBrush>
    </ResourceDictionary>

  </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>